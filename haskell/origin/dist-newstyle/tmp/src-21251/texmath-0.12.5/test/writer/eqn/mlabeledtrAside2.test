<<< native
[ EArray
    [ AlignCenter
    , AlignCenter
    , AlignCenter
    , AlignCenter
    , AlignCenter
    ]
    [ [ [ EFraction
            NormalFrac
            (EGrouped [ EIdentifier "a" , ESymbol Bin "+" , ENumber "1" ])
            (ENumber "2")
        ]
      , [ EIdentifier "b" ]
      , [ EIdentifier "c" ]
      , [ EIdentifier "d" ]
      , [ EIdentifier "e" ]
      ]
    , [ [ EIdentifier "x" ]
      , [ EGrouped
            [ EFraction NormalFrac (ENumber "2") (ENumber "3")
            , EIdentifier "y"
            , ESymbol Bin "+"
            , ENumber "5"
            ]
        ]
      , [ EIdentifier "z" ]
      , [ EIdentifier "d" ]
      , [ EIdentifier "e" ]
      ]
    , [ [ ENumber "1" ]
      , [ ENumber "2" ]
      , [ ENumber "5" ]
      , [ EIdentifier "d" ]
      , [ EIdentifier "e" ]
      ]
    ]
]
>>> eqn
matrix{
ccol{ {{a + 1} over 2} above x above 1 }
ccol{ b above {2 over 3 y + 5} above 2 }
ccol{ c above z above 5 }
ccol{ d above d above d }
ccol{ e above e above e }
}
