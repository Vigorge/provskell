<<< native
[ EFraction
    NormalFrac
    (EGrouped
       [ EIdentifier "x"
       , ESymbol Bin "+"
       , EIdentifier "y"
       , ESymbol Bin "+"
       , EIdentifier "z"
       ])
    (EGrouped
       [ EIdentifier "x"
       , EPhantom (EGrouped [ ESymbol Bin "+" , EIdentifier "y" ])
       , ESymbol Bin "+"
       , EIdentifier "z"
       ])
]
>>> mml
<?xml version='1.0' ?>
<math display="block" xmlns="http://www.w3.org/1998/Math/MathML">
  <mfrac>
    <mrow>
      <mi>x</mi>
      <mo>+</mo>
      <mi>y</mi>
      <mo>+</mo>
      <mi>z</mi>
    </mrow>
    <mrow>
      <mi>x</mi>
      <mphantom>
        <mrow>
          <mo>+</mo>
          <mi>y</mi>
        </mrow>
      </mphantom>
      <mo>+</mo>
      <mi>z</mi>
    </mrow>
  </mfrac>
</math>
